@inherits LayoutComponentBase

<TopRowBluehillLayout HeadLinks="@HeadLinks" AfterHeadLinks="@AfterHeadLinks" Body="@Body">
    <BeforeHeadLinks>
        <CultureSelector/>
        @BeforeHeadLinks
    </BeforeHeadLinks>
</TopRowBluehillLayout>

@code {
    [Parameter]
    public RenderFragment? BeforeHeadLinks { get; set; }

    [Parameter]
    public IEnumerable<HeadLink> HeadLinks { get; set; } = Array.Empty<HeadLink>();

    [Parameter]
    public RenderFragment? AfterHeadLinks { get; set; }
}
